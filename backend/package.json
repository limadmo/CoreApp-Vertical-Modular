{
  "name": "coreapp-backend",
  "version": "1.0.0",
  "description": "CoreApp Backend - Express.js + TypeScript + Prisma",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "dev:nodemon": "nodemon",
    "dev:ts": "ts-node --transpile-only src/index.ts",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "start": "node dist/index.js",
    "start:prod": "NODE_ENV=production node dist/index.js",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:seed": "tsx prisma/seeds/seed.ts",
    "db:seed:complete": "tsx prisma/seedComplete.ts",
    "db:seed:full": "npm run db:seed && npm run db:seed:complete",
    "db:reset": "prisma migrate reset --force && npm run db:seed",
    "db:reset:full": "prisma migrate reset --force && npm run db:seed:full",
    "db:generate": "prisma generate",
    "db:studio": "prisma studio",
    "lint": "echo 'Linting not configured yet'",
    "test": "echo 'Tests not configured yet'"
  },
  "keywords": [
    "express",
    "typescript",
    "prisma",
    "multi-tenant"
  ],
  "author": "Diego",
  "license": "MIT",
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.16",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "nodemon": "^3.1.10",
    "prisma": "^5.9.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@faker-js/faker": "^8.4.0",
    "@prisma/client": "^5.9.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.10",
    "axios": "^1.12.1",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "node-cache": "^5.1.2",
    "prisma-dbml-generator": "^0.12.0",
    "prisma-docs-generator": "^0.8.0",
    "prisma-json-schema-generator": "^5.1.5",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  }
}
